<UserControl x:Class="wtPay.usercontrol.CountDownTimer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:wtPay.usercontrol"
             mc:Ignorable="d" Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded" 
            >
    <Grid Margin="0,0,15,0" Height="55px" Width="100px">
        <Image Source="../cut-2/定时器黄色.png"  VerticalAlignment="Center" HorizontalAlignment="Center"  Stretch="Fill" ></Image>
        <Image Source="../cut-2/表.png"  VerticalAlignment="Center" HorizontalAlignment="Center" Height="40px" Margin="-50,0,0,0"></Image>
        <TextBlock Name="countText" Text="300" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="28" Foreground="White" Margin="0,0,4,0"></TextBlock>
    </Grid>
</UserControl>
