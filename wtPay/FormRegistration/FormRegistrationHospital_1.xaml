                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name5" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="2" Grid.Column="2" Name="button6">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name6" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="3" Grid.Column="0" Name="button7">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name7" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="3" Grid.Column="1" Name="button8">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name8" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="3" Grid.Column="2" Name="button9">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name9" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="4" Grid.Column="0" Name="button10">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name10" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="4" Grid.Column="1" Name="button11">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name11" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
            <Grid Grid.Row="4" Grid.Column="2" Name="button12">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name12" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
           











        </Grid>
        <Grid Grid.Row="3" Grid.Column="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Button Uid="公积金贷款余额查询" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center"   FontSize="1pt" Margin="40,0,0,0"   Width="150" Height="65" Click="Button_Click_1" >
                <Button.Template>
                    <ControlTemplate>
                        <Grid>
                            <Rectangle>
                                <Rectangle.Fill>
                                    <ImageBrush ImageSource="/cut-2/组 401131211.png"></ImageBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18pt" Foreground="#FFF9F3F3"  ><Run Language="zh-cn" Text="上一页"/></TextBlock>
                        </Grid>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button Uid="公积金贷款余额查询" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center"   FontSize="1pt"   Width="150" Height="65" Click="Button_Click" >
                <Button.Template>
                    <ControlTemplate>
                        <Grid>
                            <Rectangle>
                                <Rectangle.Fill>
                                    <ImageBrush ImageSource="/cut-2/组 401131211.png"></ImageBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18pt" Foreground="#FFF9F3F3"  ><Run Language="zh-cn" Text="下一页"/></TextBlock>
                        </Grid>
                    </ControlTemplate>
                </Button.Template>
            </Button>
            <Button Uid="返回" Grid.Row="0" Grid.Column="2" DataContext="cut-2/exit.png" Style="{StaticResource choose-btn-style }" Click="退出_Click">

            </Button>
        </Grid>
    </Grid>
</UserControl>

