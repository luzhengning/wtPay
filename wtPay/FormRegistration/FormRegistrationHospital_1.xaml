            </Grid>
            <Grid Grid.Row="2" Grid.Column="0" Name="button4">
                <Image Source="../cut-2/blue_1.png"  Stretch="Fill" Width="240" Height="70"></Image>
                <TextBlock Padding="5,5,5,5" TextWrapping="Wrap" Name="name4" Width="240" Height="70" FontSize="16pt" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" MouseLeftButtonDown="name1_MouseLeftButtonDown">------</TextBlock>
            </Grid>
